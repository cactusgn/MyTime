<UserControl x:Class="Summary.Domain.TimeUpdate"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Summary.Domain"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <WrapPanel>
            <TextBlock Margin="5">日期</TextBlock>
            <TextBox Width="100">{Binding CreatedDate}</TextBox>
        </WrapPanel>
        <WrapPanel>
            <TextBlock Margin="5">开始时间</TextBlock>
            <TextBox Width="100"></TextBox>
        </WrapPanel>
        <WrapPanel>
            <TextBlock Margin="5">结束时间</TextBlock>
            <TextBox Width="100"></TextBox>
        </WrapPanel>
        <WrapPanel>
            <TextBlock Margin="5">记录</TextBlock>
            <TextBox Width="100"></TextBox>
        </WrapPanel>
        <WrapPanel>
            <TextBlock Margin="5">类型</TextBlock>
            <TextBox Width="100"></TextBox>
        </WrapPanel>
    </StackPanel>
</UserControl>
